{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["./assets/*"],
      // "@components/*": ["./src/components/*"],
      // "@contexts/*": ["./src/contexts/*"],
      // "@graphql/*": ["./src/graphql/*"],
      // "@hooks/*": ["./src/hooks/*"],
      // "@pages/*": ["./src/pages/*"],
      // "@stores/*": ["./src/stores/*"],
      // "@styles/*": ["./src/styles/*"],
      // "@utils/*": ["./src/utils/*"],
      // "@axios/*": ["./src/axios/*"],
      // "@utils/*": ["./src/utils/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
